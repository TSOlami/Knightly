{
  "expo": {
    "name": "Knightly",
    "scheme": "knightly",
    "slug": "knightly-chess-puzzles",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4a6ea9"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4a6ea9"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A chess puzzle app featuring checkmate in one move puzzles",
    "githubUrl": "https://github.com/username/knightly"
  }
}
